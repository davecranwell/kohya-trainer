{
    "name": "my-remix-app",
    "private": true,
    "sideEffects": false,
    "type": "module",
    "scripts": {
        "build": "npm-run-all build:*",
        "build:remix": "NODE_ENV=production remix vite:build",
        "build:server": "NODE_ENV=production npx esbuild server/index.ts --outfile=build/index.cjs --platform=node --target=node22 --format=cjs --bundle --external:lightningcss --external:esbuild",
        "dev": "tsx --trace-warnings ./server/index.ts",
        "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
        "start": "NODE_ENV=production node ./build/index.cjs",
        "typecheck": "tsc",
        "prisma:migrate": "prisma migrate dev",
        "prisma:seed": "prisma migrate reset --force",
        "docker:build": "docker build -t webapp:latest ."
    },
    "dependencies": {
        "@auth/core": "^0.37.4",
        "@aws-sdk/client-lambda": "^3.709.0",
        "@aws-sdk/client-s3": "^3.701.0",
        "@aws-sdk/client-sqs": "^3.699.0",
        "@aws-sdk/lib-storage": "^3.701.0",
        "@aws-sdk/s3-request-presigner": "^3.717.0",
        "@aws-sdk/types": "^3.696.0",
        "@conform-to/react": "^1.2.2",
        "@conform-to/zod": "^1.2.2",
        "@headlessui/react": "^2.2.0",
        "@paralleldrive/cuid2": "^2.2.2",
        "@prisma/client": "^5.22.0",
        "@radix-ui/react-checkbox": "^1.1.2",
        "@radix-ui/react-icons": "^1.3.2",
        "@radix-ui/react-label": "^2.1.0",
        "@radix-ui/react-tooltip": "^1.1.4",
        "@remix-run/node": "^2.13.1",
        "@remix-run/react": "^2.13.1",
        "@remix-run/serve": "^2.13.1",
        "@types/bcryptjs": "^2.4.6",
        "address": "^2.0.3",
        "axios": "^1.7.7",
        "bcryptjs": "^2.4.3",
        "chalk": "^5.3.0",
        "class-variance-authority": "^0.7.0",
        "close-with-grace": "^2.1.0",
        "clsx": "^2.1.1",
        "dotenv": "^16.4.5",
        "express-rate-limit": "^7.4.1",
        "helmet": "^8.0.0",
        "input-otp": "^1.4.1",
        "isbot": "^4.1.0",
        "jsonc-parser": "^3.3.1",
        "node-cron": "^3.0.3",
        "pino": "^9.6.0",
        "pino-http": "^10.4.0",
        "pino-pretty": "^13.0.0",
        "prisma": "^5.22.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "remix-auth": "^3.7.0",
        "remix-auth-form": "^1.5.0",
        "remix-auth-google": "^2.0.0",
        "remix-auth-socials": "^2.1.0",
        "remix-utils": "^7.7.0",
        "sonner": "^1.7.0",
        "spin-delay": "^2.0.1",
        "tiny-invariant": "^1.3.3"
    },
    "devDependencies": {
        "@remix-run/dev": "^2.13.1",
        "@types/compression": "^1.7.5",
        "@types/express": "^5.0.0",
        "@types/morgan": "^1.9.9",
        "@types/node-cron": "^3.0.11",
        "@types/react": "^18.2.20",
        "@types/react-dom": "^18.2.7",
        "@typescript-eslint/eslint-plugin": "^8.18.2",
        "@typescript-eslint/parser": "^8.18.2",
        "autoprefixer": "^10.4.19",
        "esbuild": "^0.24.2",
        "eslint": "^9.17.0",
        "eslint-import-resolver-typescript": "^3.7.0",
        "eslint-plugin-import": "^2.31.0",
        "eslint-plugin-jsx-a11y": "^6.10.2",
        "eslint-plugin-react": "^7.37.3",
        "eslint-plugin-react-hooks": "^5.1.0",
        "npm-run-all": "^4.1.5",
        "postcss": "^8.4.38",
        "prettier": "^3.4.2",
        "prettier-plugin-prisma": "^5.0.0",
        "prettier-plugin-sql": "^0.18.1",
        "prettier-plugin-tailwindcss": "^0.6.5",
        "tailwindcss": "^3.4.4",
        "tailwindcss-aria-attributes": "^2.0.1",
        "tsx": "^4.19.2",
        "typescript": "^5.7.2",
        "vite": "^5.1.0",
        "vite-tsconfig-paths": "^5.1.4"
    },
    "prisma": {
        "seed": "tsx ./prisma/seed.ts"
    },
    "engines": {
        "node": "22"
    }
