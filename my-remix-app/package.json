        "@radix-ui/react-label": "^2.1.0",
        "@remix-run/node": "^2.13.1",
        "@remix-run/react": "^2.13.1",
        "@remix-run/serve": "^2.13.1",
        "@types/bcryptjs": "^2.4.6",
        "address": "^2.0.3",
        "axios": "^1.7.7",
        "bcryptjs": "^2.4.3",
        "chalk": "^5.3.0",
        "class-variance-authority": "^0.7.0",
